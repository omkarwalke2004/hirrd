{
    "rewrites": [
      {
        "source": "/onboarding",
        "destination": "/"
      },
      {
        "source": "/jobs",
        "destination": "/"
      },
      {
        "source": "/job/:id",
        "destination": "/"
      },
      {
        "source": "/post-job",
        "destination": "/"
      },
      {
        "source": "/saved-job",
        "destination": "/"
      },
      {
        "source": "/my-jobs",
        "destination": "/"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
          { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
        ]
      }
    ]
  }
  